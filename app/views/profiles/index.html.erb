<h2>Users that joined</h2>

<table style="width: 100%">
  <thead>
  <tr>
    <th>User</th>
    <th># Bookmarks</th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.bookmarks.count %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<%#= paginate @users %>